{
  "name": "tsparticles-clarity-plugin",
  "version": "1.0.1",
  "private": true,
  "author": "Matteo Bruni",
  "license": "MIT",
  "keywords": [
    "typedoc",
    "plugin",
    "theme",
    "typedoctheme",
    "typedoc-theme"
  ],
  "bugs": {
    "url": "https://github.com/matteobruni/tsparticles-clarity-plugin/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matteobruni/tsparticles-clarity-plugin.git"
  },
  "scripts": {
    "build": "yarn clear:dist && yarn prettify:src && yarn lint && tsc && yarn distfiles",
    "clear:dist": "rimraf -f ./dist",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "prettify:src": "prettier --write ./src",
    "distfiles": "node ./scripts/distfiles.js"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "typedoc": "^0.22.10"
  },
  "dependencies": {
    "typescript": "<4.5.0"
  }
}
