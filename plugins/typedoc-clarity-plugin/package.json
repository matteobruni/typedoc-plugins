{
  "name": "typedoc-plugin-clarity",
  "version": "1.1.0",
  "private": true,
  "author": "Matteo Bruni",
  "license": "MIT",
  "keywords": [
    "typedoc",
    "plugin",
    "typedocplugin",
    "typedoc-plugin"
  ],
  "bugs": {
    "url": "https://github.com/matteobruni/typedoc-clarity-plugin/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matteobruni/typedoc-clarity-plugin.git"
  },
  "scripts": {
    "build": "yarn clear:dist && yarn prettify:src && yarn lint && tsc && yarn distfiles",
    "clear:dist": "rimraf -f ./dist",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "prettify:src": "prettier --write ./src",
    "distfiles": "node ./scripts/distfiles.js"
  },
  "peerDependencies": {
    "typedoc": "^0.23.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "typedoc": "^0.23.5"
  },
  "dependencies": {
    "typescript": "^4.7.4"
  }
}
