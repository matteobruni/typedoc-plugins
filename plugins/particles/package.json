{
  "name": "typedoc-plugin-particles",
  "version": "1.0.0",
  "private": true,
  "author": "Matteo Bruni",
  "license": "MIT",
  "keywords": [
    "typedoc",
    "plugin",
    "typedocplugin",
    "typedoc-plugin"
  ],
  "bugs": {
    "url": "https://github.com/matteobruni/typedoc-plugins/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matteobruni/typedoc-plugins.git",
    "directory": "particles"
  },
  "scripts": {
    "build": "yarn clear:dist && yarn prettify:src && yarn lint && tsc && yarn distfiles",
    "clear:dist": "rimraf ./dist",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "prettify:src": "prettier --write ./src",
    "distfiles": "node ../../scripts/distfiles.js"
  },
  "publishConfig": {
    "directory": "dist",
    "linkDirectory": true
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "typedoc": "^0.24.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^2.8.7",
    "rimraf": "^5.0.0",
    "typedoc": "^0.24.2"
  },
  "dependencies": {
    "typescript": "^5.0.4"
  }
}
