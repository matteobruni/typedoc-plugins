{
  "name": "typedoc-plugin-carbon-ads",
  "version": "1.3.0",
  "author": "Matteo Bruni",
  "license": "MIT",
  "keywords": [
    "typedoc",
    "plugin",
    "typedocplugin",
    "typedoc-plugin"
  ],
  "bugs": {
    "url": "https://github.com/matteobruni/typedoc-plugins/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matteobruni/typedoc-plugins.git",
    "directory": "carbon-ads"
  },
  "scripts": {
    "build": "yarn clear:dist && yarn prettify:src && yarn lint && tsc && yarn distfiles",
    "clear:dist": "rimraf ./dist",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "prettify:src": "prettier --write ./src",
    "distfiles": "node ../../scripts/distfiles.js",
    "version": "pnpm run distfiles && git add package.dist.json",
    "prepack": "pnpm run build"
  },
  "publishConfig": {
    "directory": "dist",
    "linkDirectory": true
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "typedoc": "^0.24.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "typedoc": "^0.24.6"
  },
  "dependencies": {
    "typescript": "^5.0.4"
  }
}
